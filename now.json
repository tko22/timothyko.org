{
  "version": 2,
  "name": "timothyko.org",
  "alias": "www.timothyko.org",
  "builds": [
    {
      "src": "writing/package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "public"
      }
    },
    {
      "src": "www/**",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/writing/(.*)",
      "dest": "/writing/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/www/$1"
    }
  ]
}